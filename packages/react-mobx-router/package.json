    "mobx-react-lite": "4.0.7",
    "react": "18.2.0"
  },
  "peerDependencies": {
    "mobx": "*",
    "mobx-react-lite": "*",
    "react": "*"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
