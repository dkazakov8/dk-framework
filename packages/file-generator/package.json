  "nyc": {
    "include": "src"
  },
  "dependencies": {
    "chalk": "4.1.1",
    "chokidar": "3.5.3",
    "fs-extra": "10.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "ts-interface-checker": "1.0.2"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "gitHead": "f431f3564e833816fcc5e0124c21b3948554747f"
}
