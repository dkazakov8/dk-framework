{
  "name": "dk-file-generator",
  "author": "Dmitry Kazakov",
  "license": "MIT",
  "version": "3.0.0",
  "description": "Creates helper files",
  "scripts": {
    "upd": "rm -rf ./node_modules&&pnpm i --prefer-offline",
    "build": "rm -rf ./dist&&tsc --project tsconfig-compile.json",
