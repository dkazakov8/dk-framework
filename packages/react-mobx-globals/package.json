    "react": "19.1.1",
    "ts-interface-checker": "1.0.2"
  },
  "peerDependencies": {
    "dk-localize": "*",
    "dk-request": "*",
    "mobx": "*",
    "react": "*"
  },
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "main": "dist/cjs/index.js",
