  },
  "nyc": {
    "include": "src"
  },
  "devDependencies": {
    "lodash": "4.17.21",
    "mobx": "6.12.3"
  },
  "peerDependencies": {
    "mobx": "*"
  },
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "gitHead": "4e864f69d1f82a93f53233cf5669fe13d023d132"
