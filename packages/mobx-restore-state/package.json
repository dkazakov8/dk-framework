  "license": "MIT",
  "version": "3.3.7",
  "description": "Restores state from initial data object",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkazakov8/dk-framework.git"
  },
  "scripts": {
    "build": "rm -rf ./dist&&tsc --project tsconfig-compile.json&&node ../../bundle.js",
