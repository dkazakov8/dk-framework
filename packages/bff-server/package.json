    "serve-static": "1.15.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "gitHead": "f431f3564e833816fcc5e0124c21b3948554747f"
