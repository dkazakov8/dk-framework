  },
  "devDependencies": {
    "mobx": "6.12.3",
    "mobx-react-lite": "4.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependencies": {
    "mobx": "*",
    "react": "*"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
