    "dk-checker-remove-extraneous": "3.0.0"
  },
  "devDependencies": {
    "nock": "13.2.8",
    "ts-interface-checker": "1.0.2"
  },
  "peerDependencies": {
    "ts-interface-checker": "*"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "gitHead": "f431f3564e833816fcc5e0124c21b3948554747f"
